[global_config]
  enabled_plugins = LaunchpadCodeURLHandler, APTURLHandler, LaunchpadBugURLHandler
  tab_position = hidden
  title_transmit_bg_color = "#f50adf"
[keybindings]
  split_horiz = <Primary><Shift>d
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
[profiles]
  [[default]]
    cursor_color = "#2dcde0"
    font = Operator Mono weight=330 18
    foreground_color = "#2dcde0"
    palette = "#073642:#cb2aec:#ffffff:#ec0bc0:#268bd2:#d33682:#ffffff:#eee8d5:#002b36:#cb1651:#ffffff:#ff0016:#969696:#6c71c4:#8000ad:#fdf6e3"
    show_titlebar = False
    use_system_font = False
    visible_bell = True
  [[Solarized]]
    background_color = "#002b36"
    foreground_color = "#839496"
    palette = "#073642:#dc322f:#859900:#b58900:#268bd2:#d33682:#2aa198:#eee8d5:#002b36:#cb4b16:#586e75:#657b83:#839496:#6c71c4:#93a1a1:#fdf6e3"
  [[monokai]]
    allow_bold = False
    background_color = "#0d0d0d"
    font = DejaVu Sans Mono 10
    foreground_color = "#ffffff"
    palette = "#1B1B1D1D1E1E:#F9F926267272:#8282B4B41414:#FDFD97971F1F:#5656C2C2D6D6:#8C8C5454FEFE:#464654545757:#CCCCCCCCC6C6:#505053535454:#FFFF59599595:#B6B6E3E35454:#FEFEEDED6C6C:#8C8CEDEDFFFF:#9E9E6F6FFEFE:#89899C9CA1A1:#F8F8F8F8F2F2"
    use_system_font = False
